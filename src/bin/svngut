#!/usr/bin/python

def set_python_path():
    import sys
    import os
    path_to_svngut = os.path.realpath(os.getcwd()+'/../')
    path_to_etc = os.path.realpath(os.getcwd()+'/../etc/')
    sys.path.insert(0, path_to_etc)
    sys.path.insert(0, path_to_svngut)
    
set_python_path()
import svngut.engine